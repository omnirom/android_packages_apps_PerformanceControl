<?xml version="1.0" encoding="utf-8"?><!--
File contain partitions layout for flashing kernel & recovery
-->
<devices>
    <device>
        <model>GT-I9001,Galaxy S Plus,GT-I8150,SGH-T679</model>
        <!-- GALAXY S PLUS,GALAXY W,Galaxy Exhibit -->
        <kernel>/dev/block/mmcblk0p8</kernel>
        <recovery>/dev/block/mmcblk0p13</recovery>
        <cache>/dev/block/mmcblk0p16</cache>
    </device>
    <device>
        <model>GT-I9100,GT-I9100G,SGH-I777</model>
        <!-- GALAXY S II -->
        <kernel>/dev/block/mmcblk0p5</kernel>
        <recovery>/dev/block/mmcblk0p6</recovery>
        <cache>/dev/block/mmcblk0p7</cache>
    </device>
    <device>
        <model>GT-I9300</model>
        <!-- GALAXY S III -->
        <kernel>/dev/block/mmcblk0p5</kernel>
        <recovery>/dev/block/mmcblk0p6</recovery>
        <cache>/dev/block/mmcblk0p8</cache>
    </device>
    <!-- GALAXY S IV,NEXUS 4,XPERIA Z,Samsung Galaxy S4 SGH-M919 -->
    <device>
        <model>GT-I9505,Nexus 4,LG-E960,C6603,SGH-M919</model>
        <kernel>/dev/block/platform/msm_sdcc.1/by-name/boot</kernel>
        <recovery>/dev/block/platform/msm_sdcc.1/by-name/recovery</recovery>
        <cache>/dev/block/platform/msm_sdcc.1/by-name/cache</cache>
    </device>
    <device>
        <model>Galaxy Nexus,GT-I9250</model>
        <!-- GALAXY NEXUS -->
        <kernel>/dev/block/platform/omap/omap_hsmmc.0/by-name/boot</kernel>
        <recovery>/dev/block/platform/omap/omap_hsmmc.0/by-name/recovery</recovery>
        <cache>/dev/block/platform/omap/omap_hsmmc.0/by-name/cache</cache>
    </device>
    <device>
        <model>HTC One X,S720e</model>
        <!-- HTC ONE X -->
        <kernel>/dev/block/mmcblk0p4</kernel>
        <recovery>/dev/block/mmcblk0p5</recovery>
        <cache>/dev/block/mmcblk0p13</cache>
    </device>
    <!-- new added 1-->
    <device>
        <model>GT-I8190</model>
        <!-- GALAXY S III mini -->
        <kernel>/dev/block/mmcblk0p20</kernel>
        <recovery>/dev/block/mmcblk0p21</recovery>
        <cache>/dev/block/mmcblk0p23</cache>
    </device>
    <!-- Samsung Galaxy Tab 7.7 (3G) GT-P6800, GT-P6810 -->
    <device>
        <model>GT-P6800,GT-P6810</model>
        <kernel>/dev/block/mmcblk0p5</kernel>
        <recovery>/dev/block/mmcblk0p6</recovery>
        <cache>/dev/block/mmcblk0p7</cache>
    </device>
    <!-- Samsung Galaxy S III LTE GT-I9305 -->
    <device>
        <model>GT-I9305</model>
        <kernel>/dev/block/mmcblk0p8</kernel>
        <recovery>/dev/block/mmcblk0p9</recovery>
        <cache>/dev/block/mmcblk0p12</cache>
    </device>
    <!-- Samsung Galaxy S III SGH-T999,SGH-I747 -->
    <device>
        <model>SGH-T999,SGH-I747,SGH-I747,SGH-I747M,SPH-L710,SCH-I535</model>
        <kernel>/dev/block/mmcblk0p7</kernel>
        <recovery>/dev/block/mmcblk0p18</recovery>
        <cache>/dev/block/mmcblk0p17</cache>
    </device>
    <!-- Samsung Galaxy Tab 10.1 -->
    <device>
        <model>GT-P7500,SCH-I905,GT-P7510,GT-P7100,GT-P7511</model>
        <kernel>/dev/block/mmcblk0p3</kernel>
        <recovery>/dev/block/mmcblk0p2</recovery>
