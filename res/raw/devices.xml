    </device>
    <device>
        <model>LG-P970</model>
        <!-- LG OPTIMUS BLACK P970 -->
        <kernel>/dev/block/mmcblk0p3</kernel>
        <recovery>/dev/block/mmcblk0p4</recovery>
        <cache>/dev/block/mmcblk0p10</cache>
    </device>
    <!-- New devices added by rodero95 -->
    <device>
        <model>GT-I9003,GT-I9003L,galaxysl</model>
        <!-- Samsung Galaxy SL i9003 and i9003L -->
        <kernel>/dev/block/mtdblock0</kernel>
        <recovery>/dev/block/mtdblock1</recovery>
        <cache>/dev/block/mmcblk0p2</cache>
    </device>
    <device>
        <model>u8815</model>
        <!-- Huawei Ascend G300 -->
        <kernel>/dev/block/mmcblk0p15</kernel>
        <recovery>/dev/block/mmcblk0p16</recovery>
